{
  "name": "Misinformation Detector MSc Research",
  "description": "A web extension to try identify misinformation by analysing textual content",
  "version": "0.2",
  "manifest_version": 3,
    "background": {
    "service_worker": "background.js"
  }, 
    "permissions": ["tabs"],
    "host_permissions": [],
      "action": {
        "default_popup": "popup.html",
        "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
    "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }

}